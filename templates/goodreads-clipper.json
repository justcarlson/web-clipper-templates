{
	"schemaVersion": "0.1.1",
	"name": "Goodreads",
	"behavior": "create",
	"noteNameFormat": "{{selector:.BookPageTitleSection__title h1}}",
	"path": "Clippings",
	"noteContentFormat": "{{selector:.BookPageMetadataSection__description}}",
	"properties": [
		{
			"name": "categories",
			"value": "Clippings,Books",
			"type": "multitext"
		},
		{
			"name": "author",
			"value": "{{schema:author[*].name|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "cover",
			"value": "{{schema:image}}",
			"type": "text"
		},
		{
			"name": "genre",
			"value": "{{selector:.BookPageMetadataSection__genres .BookPageMetadataSection__genreButton|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "pages",
			"value": "{{schema:numberOfPages}}",
			"type": "number"
		},
		{
			"name": "isbn",
			"value": "{{schema:isbn}}",
			"type": "text"
		},
		{
			"name": "year",
			"value": "{{selector:p[data-testid='publicationInfo']|split:\"First published \"|slice:1|date:\"YYYY\"}}",
			"type": "number"
		},
		{
			"name": "rating",
			"value": "",
			"type": "number"
		},
		{
			"name": "topics",
			"value": "",
			"type": "multitext"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "last",
			"value": "",
			"type": "date"
		},
		{
			"name": "via",
			"value": "Goodreads",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "books, to-read",
			"type": "multitext"
		}
	],
	"triggers": [
		"https://www.goodreads.com/book/"
	]
}
