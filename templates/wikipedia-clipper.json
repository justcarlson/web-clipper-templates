{
	"schemaVersion": "0.1.0",
	"name": "Wikipedia",
	"behavior": "create",
	"noteNameFormat": "{{title}}",
	"path": "Clippings",
	"noteContentFormat": "{{selectorHtml:#mw-content-text|remove_html:(\".navbox,.printfooter,.side-box\")|markdown}}",
	"properties": [
		{
			"name": "categories",
			"value": "Clippings",
			"type": "multitext"
		},
		{
			"name": "author",
			"value": "[[Wikipedia]]",
			"type": "multitext"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "topics",
			"value": "",
			"type": "multitext"
		},
		{
			"name": "tags",
			"value": "clippings, wikipedia",
			"type": "multitext"
		}
	],
	"triggers": [
		"/^https:\\/\\/[a-z]{2,3}\\.wikipedia\\.org\\/wiki\\/?$/"
	]
}
