{
	"schemaVersion": "0.1.0",
	"name": "ChatGPT",
	"behavior": "create",
	"noteNameFormat": "{{title}}",
	"path": "Clippings",
	"noteContentFormat": "{{selectorHtml:article[data-testid*=\"conversation-turn\"]|replace:\"/<svg.*?svg>/g\":\"### ChatGPT\\:\"|join:\"\\n\\n\"|markdown|replace:(\"\\\\###\":\"###\", \"##### You said\": \"### You:\")}}\n",
	"properties": [
		{
			"name": "categories",
			"value": "[[Clippings]]",
			"type": "multitext"
		},
		{
			"name": "author",
			"value": "{{author|split:\", \"|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{published}}",
			"type": "date"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "tags",
			"value": "clippings, chatgpt",
			"type": "multitext"
		}
	],
	"triggers": [
		"https://chatgpt.com/share",
		"https://chatgpt.com/c"
	]
}
