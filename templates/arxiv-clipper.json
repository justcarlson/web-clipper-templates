{
	"schemaVersion": "0.1.0",
	"name": "arXiv",
	"behavior": "create",
	"noteNameFormat": "{{title}}",
	"path": "Clippings",
	"noteContentFormat": "{{content}}",
	"properties": [
		{
			"name": "categories",
			"value": "Clippings|wikilink",
			"type": "multitext"
		},
		{
			"name": "author",
			"value": "{{selector:.ltx_authors .ltx_personname|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{selector:.ltx_dates|replace:\"(\":\"\"|replace:\")\":\"\"|date:\"YYYY-MM-DD\"}}",
			"type": "date"
		},
		{
			"name": "year",
			"value": "{{selector:.ltx_dates|replace:\"(\":\"\"|replace:\")\":\"\"|date:\"YYYY\"}}",
			"type": "number"
		},
		{
			"name": "arxiv",
			"value": "{{url|split:\"/\"|slice:4|split:\"#\"|slice:0,1}}",
			"type": "text"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "tags",
			"value": "clippings, arxiv, papers",
			"type": "multitext"
		}
	],
	"triggers": [
		"https://arxiv.org/html/"
	]
}
